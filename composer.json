{
  "name": "mrmadclown/increment-file-names-plugin",
  "description": "A Flysystem Plugin to increment duplicate Filenames",
  "type": "library",
  "require": {
    "php": ">=7.2",
    "league/flysystem": "^1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "11.4.2",
    "phpbench/phpbench": "1.2.10",
    "league/flysystem-memory": "3.0.14"
  },
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "MrMadClown\\IncrementFileNames\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MrMadClown\\IncrementFileNames\\Tests\\": "tests/"
    }
  }
}
